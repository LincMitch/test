{
  "name": "HxUi",
  "private": true,
  "skpm": {
    "main": "hxui.sketchplugin",
    "manifest": "src/manifest.json"
  },
  "scripts": {
    "build": "skpm-build",
    "watch": "skpm-build --watch",
    "render": "skpm-build --watch --run",
    "render:once": "skpm-build --run",
    "postinstall": "npm run build && skpm-link",
    "web": "react run src/web.js"
  },
  "author": "Brian Pulliese <brian.pulliese@medicaldirector.com>",
  "license": "MIT",
  "dependencies": {
    "chroma-js": "^1.2.2",
    "nwb": "^0.15.6",
    "prop-types": "^15.5.8",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-native": "^0.42.3",
    "react-primitives": "^0.3.4",
    "react-sketchapp": "^1.0.0",
    "react-test-renderer": "^15.4.1",
    "styled-components": "^3.0.2",
    "webpack": "^2.5.1"
  },
  "devDependencies": {
    "@skpm/builder": "^0.2.0"
  }
}
